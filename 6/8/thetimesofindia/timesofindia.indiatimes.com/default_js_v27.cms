function setOpacity(value) {
headlines[count].style.opacity = value/10;
if (agt.indexOf("msie") != -1){
if(!headlines[count].currentStyle.hasLayout){headlines[count].style.zoom="1"}}
headlines[count].style.filter = "alpha(opacity=" + (value * 10) + ")";
fadeLevel--;
timeOut = setTimeout(fadeout, 100);
}
function latest() {headlines = document.getElementById('lnews').getElementsByTagName('li');show();}
function show() {
if ( headlines.length > 0 ) {                
headlineDisplay(count, "inline", 1, 100);
startTimeOut = setTimeout(fadeout, 3000); 
}}
function headlineDisplay(count, display, opacity, filter) {
headlines[count].style.display = display;
headlines[count].style.opacity = opacity;
headlines[count].style.filter = 'alpha(opacity=' + filter + ')';
}
function fadeout() { 
if (fadeLevel > 0) {setOpacity(fadeLevel);} 
else {clearTimeout(timeOut);clearTimeout(startTimeOut);
headlineDisplay(count, "none", 0, 0);
fadeLevel = 10;
count++;
if(count >= headlines.length) count = 0;
show();	}
}
