		{
			cntr = divid[dummy];
			for(i=0;i<cntr.childNodes.length;i++){if (cntr.childNodes[i].nodeName=='#text'){cntr.removeChild(cntr.childNodes[i]);}}
			instnode = cntr.firstChild;
			inncntr = cntr.lastChild;
			cntr.insertBefore(inncntr,instnode);				
			increment[dummy]=-(increment[dummy]);
			loop[dummy]=increment[dummy];
			divid[dummy].style.left=increment[dummy];
		}	
		divid[dummy].style.left=loop[dummy];		
		tempa=loop[dummy]+incrementleftval1[dummy];
		if(tempa<=0)
		{
			chkk=loop[dummy]+1;
			if(chkk>=0)
			{
				loop[dummy]=increment[dummy];
			}
			else
			{
				loop[dummy]=incrementleftval1[dummy]+loop[dummy];
			}
		}
		else
		{
			loop[dummy]+=incrementleftval1[dummy];
		}
		if(loop[dummy]<=0)
		{}
