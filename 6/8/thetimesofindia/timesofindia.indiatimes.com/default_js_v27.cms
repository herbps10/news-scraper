		{
			clearInterval ( sliderinetvala[dummy] );
			busya[dummy]=0;
		}
	}
	else
	{
		divid[dummy].style.left=loop[dummy];
		tempa=loop[dummy]+incrementby[dummy];
		if(tempa>=increment[dummy])
		{
			chkk=loop[dummy]+1;
			if(chkk<=increment[dummy])
			{
				loop[dummy]=increment[dummy];
			}
			else
			{
				loop[dummy]+=incrementby[dummy];
			}
		}
		else
		{
			loop[dummy]+=incrementby[dummy];
		}
		if(loop[dummy]<=increment[dummy])
		{}
		else
		{
			clearInterval ( sliderinetvala[dummy] );
			busya[dummy]=0;
		}
	}
}
function forward(dummy)
{
		divid[dummy].style.left=loop[dummy];
		tempa=loop[dummy]-incrementby[dummy];
		if(tempa<=increment[dummy])
		{
			chkk=loop[dummy]-1;



			if(chkk>=increment[dummy])
			{
				loop[dummy]=increment[dummy];
			}
			else
			{
				loop[dummy]-=incrementby[dummy];
			}
		}
		else
		{
			loop[dummy]-=incrementby[dummy];
		}
		if(loop[dummy]>=increment[dummy])
		{}
		else
		{
			clearInterval ( sliderinetvala[dummy] );
			if(slideshow[dummy]==1)
			{
				cntr = divid[dummy];
				for(i=0;i<cntr.childNodes.length;i++){if (cntr.childNodes[i].nodeName=='#text'){cntr.removeChild(cntr.childNodes[i]);}}
				instnode = cntr.firstChild;				
				cntr.appendChild(instnode);
				divid[dummy].style.left=0;
			}
			busya[dummy]=0;
		}
}
