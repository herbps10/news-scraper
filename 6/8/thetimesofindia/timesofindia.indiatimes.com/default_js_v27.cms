function nextslide(totslide,movingslideno,movepixel,bydefaultslide,divname,speed,preid,nextid,incrementleftval,countpos,slidemode)
{

	if(busya[countpos]==0)
	{	
		if(slidemode!=1)
		{
			divid[countpos]=document.getElementById(divname);}
		else
		{
			divid[countpos]=divname;
		}
		if(slidemode!=1)
		{
			clearInterval (playerinterval[countpos]);}
			maxlimit[countpos]=totslide-bydefaultslide;
			counta[countpos]=counta[countpos]+1;
			
			
			count=counta[countpos];	
			if(count<=maxlimit[countpos])
			{	
				if(slideshow[countpos]==1){counta[countpos]=counta[countpos]-1;count=counta[countpos];}
			}
			else
			{	
				if(slideshow[countpos]==1){counta[countpos]=counta[countpos]-1;count=counta[countpos];}
			}	
			
			if(count<=maxlimit[countpos])
			{	
			
				busya[countpos]=1;
				incrementleftval1[countpos]=incrementleftval;
				incrementby[countpos]=incrementleftval;
				glospeed=speed;		
				increment[countpos]=movingslideno*movepixel;	
				increment[countpos]=-(increment[countpos]);	
				already[countpos]=divid[countpos].offsetLeft;	
				already[countpos]=parseInt(already[countpos]);increment[countpos]=parseInt(increment[countpos]);
				increment[countpos]=already[countpos]+increment[countpos];
				loop[countpos]=already[countpos];	
				sliderinetvala[countpos] = setInterval("forward("+countpos+")",speed);	
				if(slideshow[countpos]!=1)
				{
					if(count>=1)
					{
						if(slidemode!=1){document.getElementById(preid).className="backon";}else{preid.className="backon"}
					}
					if(count==maxlimit[countpos])
					{
					if(slidemode!=1)
					{
						document.getElementById(nextid).className="nextoff";}else{nextid.className="nextoff"}
					}
					else
					{
						if(slidemode!=1){document.getElementById(nextid).className="nexton";document.getElementById(preid).className="backon";}else{nextid.className="nexton";preid.className="backon"}
					}
				}

			}
			else{if(slideshow[countpos]==0){counta[countpos]=maxlimit[countpos];}}
	}
}
function preslide(totslide,movingslideno,movepixel,bydefaultslide,divname,speed,preid,nextid,incrementleftval,countpos,slidemode)
{
	if(busya[countpos]==0)
		{
			if(slideshow[countpos]!=slidemode)
			{
				clearInterval (playerinterval[countpos]);
			}
			
			counta[countpos]=counta[countpos]-1;

				count=counta[countpos];		
			if(count>=0)
			{
				if(slideshow[countpos]==1){counta[countpos]=counta[countpos]+1;count=counta[countpos];}
			}
			else
			{
				if(slideshow[countpos]==1){counta[countpos]=counta[countpos]+1;count=counta[countpos];}
			}	
			
			if(count>=0)
			{
				busya[countpos]=1;
				incrementleftval1[countpos]=incrementleftval;
				incrementby[countpos]=incrementleftval;
				maxlimit[countpos]=totslide-bydefaultslide;
				glospeed=speed;
				divid[countpos]=document.getElementById(divname);
				increment[countpos]=movingslideno*movepixel;	
				already[countpos]=divid[countpos].offsetLeft;
				already[countpos]=parseInt(already[countpos]);increment[countpos]=parseInt(increment[countpos]);
				increment[countpos]=already[countpos]+increment[countpos];
				loop[countpos]=already[countpos];	
				decideback=0;
				sliderinetvala[countpos] = setInterval("backward("+countpos+")",speed);	
				if(slideshow[countpos]!=1)
					{
						if(count==0)
						{
							document.getElementById(preid).className="backoff";
						}	
						else
						{
							document.getElementById(nextid).className="nexton";document.getElementById(preid).className="backon";
						}
					}
			}
			else{if(slideshow[countpos]==0){counta[countpos]=counta[countpos]+1;}}
		}
}
