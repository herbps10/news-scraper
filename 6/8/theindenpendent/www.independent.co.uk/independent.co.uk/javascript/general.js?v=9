(function($) {
	$(document).ready(function() {

		$('div.gallery').each(function() {
			var $gallery = $(this);
			var $galleryList = $gallery.find('div.gallery-list');
			var $galleryContent = $gallery.find('div.gallery-content');
			var $galleryUl = $galleryList.find("ul");
			var t = null;
			var imgHeight = $galleryContent.find('img').height();

			if ($.browser.msie && $.browser.version <= 6 && $galleryContent.find('img').width() > 615) {
				$galleryContent.find('img').css({ width: '615px' });
			}

			if ($.cookie('gallery-autoplay') == 1) {
				$galleryContent.find('.control a.autoplay').text('Stop');
				t = setTimeout(function() {
						if ($(".gallery-list li a.active").parent().next('li').length != 0)
							window.location = $galleryContent.find('.control a.autoplay').attr('href');
						else
							$galleryContent.find('.control a.autoplay').click();
					}, 5000);
			}

			$galleryContent.find('.control a.autoplay').click(function(e) {
				e.preventDefault();

				if ($(this).text() == 'Stop') {
					clearTimeout(t);
					t = null;
					$(this).text('Autoplay');
					$.cookie('gallery-autoplay', 0);
				}else{
					$(this).text('Stop');
					var href = $(this).attr('href');
					t = setTimeout(function() {
						window.location = href;
					}, 5000);
					$.cookie('gallery-autoplay', 1);
				}
			});

			$galleryList.find('li a').click(function() {
				if (!t)
					return;
				clearTimeout(t);
				t = null;
				$galleryContent.find('.control a.autoplay').text('Autoplay');
				$.cookie('gallery-autoplay', 0);
			});

			if (imgHeight < 421 && $.browser.msie && $.browser.version <= 6)
				$galleryContent.find('p.image').css({ height: 420 + 'px' });

			if (imgHeight < 421 && !$.browser.msie && imgHeight > 0) {
				$galleryContent.find('p.image').css({ height: 421 + 'px' });
				$galleryContent.find('img').css({ marginTop: ((421 - imgHeight) / 2) + 'px' });
			}

			$gallery.find('iframe').attr('allowtransparency', 'true');

			var prev = function() {
				var newMargin = parseInt($galleryUl.css('marginLeft'), 10) - (max * -105);

				if (newMargin >= -10) {
					newMargin = -10;
					$galleryList.find('p.next').removeClass('disabled');
					$galleryList.find('p.prev').addClass('disabled');
				}else
					$galleryList.find('p').removeClass('disabled');

				$galleryUl.animate({ marginLeft: newMargin + 'px' })
			};

			var next = function() {
				var newMargin = parseInt($galleryUl.css('marginLeft'), 10) - (max * 105);

				if (newMargin <= maxWidth) {
					newMargin = maxWidth;
					$galleryList.find('p.next').addClass('disabled');
					$galleryList.find('p.prev').removeClass('disabled');
				}else
					$galleryList.find('p').removeClass('disabled');

				$galleryUl.animate({ marginLeft: newMargin + 'px' })
			};

			var len = $galleryList.find('li').length;
			$galleryUl.css({ width: (len * 105) + "px" });

			var max = 9;
			var maxWidth = -((len - max) * 105) -10;

			if (len > max)
				$galleryList.append('<p class="prev disabled"><a href="#">prev</a></p><p class="next"><a href="#">next</a></p>');

			if (parseInt($galleryUl.css('marginLeft')) < -10)
				$galleryList.find('p.prev').removeClass('disabled');

			if (parseInt($galleryUl.css('marginLeft')) <= maxWidth)
				$galleryList.find('p.next').addClass('disabled');

			$galleryList.find('p a').click(function(e) {
				e.preventDefault();
				($(this).parent().hasClass('next')) ? next() : prev();
			});

		});
	});

})(jQuery);

